                                </a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("Create", "PageContent")">
                                            <i class="metismenu-icon">
                                            </i>Create Page
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "PageContent")">
                                            <i class="metismenu-icon">
                                            </i>View / Edit Pages
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "NewsArticle")">
                                            <i class="metismenu-icon">
                                            </i>News / Articles
                                        </a>
                                    </li>
                                    @*<li>
                                        <a href="@Url.Action("Index", "StaffDuty")">
                                            <i class="metismenu-icon">
                                            </i>Staff Details
                                        </a>
                                    </li>*@
                                </ul>
                            </li>
                            <li>
                                <a href="#">
                                    <i class="metismenu-icon pe-7s-news-paper"></i>
                                    Media
                                    <i class="metismenu-state-icon pe-7s-angle-down caret-left"></i>
                                </a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("Index", "MediaCategoryMaster")">
                                            <i class="metismenu-icon">
                                            </i>Media Categories
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "MediaGallery")">
                                            <i class="metismenu-icon">
                                            </i>Media Gallery
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li class="app-sidebar__heading">Security</li>
                            <li>
                                <a href="#">
                                    <i class="metismenu-icon pe-7s-car"></i>
                                    Administration
                                    <i class="metismenu-state-icon pe-7s-angle-down caret-left"></i>
                                </a>
                                <ul>
                                    <li>
                                        <a href="@Url.Action("Register", "Account")">
                                            <i class="metismenu-icon">
                                            </i>Create Users
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Create", "MenuRole")">
                                            <i class="metismenu-icon">
                                            </i>Menu Permissions
                                        </a>
                                    </li>
                                    <li>
                                        <a href="@Url.Action("Index", "ContactUs")">
                                            <i class="metismenu-icon">
                                            </i>Contact Us / Grievances
                                        </a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="@Url.Action("Index", "UserActivity")">
                                    <i class="metismenu-icon pe-7s-notebook"></i>
                                    Audit Log
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="app-main__outer">
                <div class="app-main__inner" id="dvcaptcha">
                    @Html.Partial("_Notifications")
                    @RenderBody()
                </div>
                <div class="app-wrapper-footer">
